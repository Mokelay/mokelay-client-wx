<!--buildingblock/bb-indep-QR/bb-indep-QR.wxml-->
<view class="bb-indep-QR">
  <view class="container-box">
      <view class="img-box">
          <image bindtap="previewImg" style="{{sizeStyle}}" mode="scaleToFill" src="{{imagePath}}"></image>
      </view>
  </view>
  <view class="canvas-box">
    <canvas  hidden="{{canvasHidden}}" style="{{sizeStyle}}" canvas-id="mycanvas"/>
  </view>
</view>
