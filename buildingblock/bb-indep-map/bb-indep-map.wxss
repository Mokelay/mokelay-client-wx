/* buildingblock/bb-indep-map/bb-indep-map.wxss */

@import "../../lib/iconfont/iconfont.wxss";

.bb-indep-map{
  position: relative;
  width: 100%;
  min-height:300px;
}
.coverReset{
  width:30px;
  height:30px;
  border-radius:5px;
  background:#fff;
  box-shadow:1px 1px 1px #ccc;
  bottom:40px;
  position:absolute;
  left:15px;
}
.coverReset .icon{
  margin: 0 auto;
  margin-top: 5px;
  width: 20px;
  height: 20px;
  background:#409EFF;
  border-radius: 50%;
}
.coverReset .iconfont{
  text-align:center;
  line-height:30px;
  color:#409EFF;
  font-size:24px;
}