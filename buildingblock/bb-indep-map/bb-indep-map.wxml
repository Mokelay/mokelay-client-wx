<!--buildingblock/bb-indep-map/bb-indep-map.wxml-->
<map
      id="myMap"
      class="bb-indep-map"
      style="{{domStyle}}"
      latitude="{{latitude}}"
      longitude="{{longitude}}"
      scale="{{scale}}"
      showLocation="{{showLocation}}"
      enableZoom="{{enableZoom}}"
      enableScroll="{{enableScroll}}"
      markers="{{markers}}"
      show-location
    >
    <cover-view class="coverReset" bindtap='coverReset'>
      <cover-view class='icon'></cover-view>
      <!-- <cover-view class='iconfont ty-dlweizhi'></cover-view> -->
    </cover-view>
    </map>
