@import "../../lib/iconfont/iconfont.wxss";
.bb-vant-product-list {
  background: #f5f5f5;
}
.bb-vant-product-list .van-checkbox__icon {
  margin-left: 20rpx;
  margin-bottom: 100rpx;
}
.bb-vant-product-list .van-checkbox__label {
  width: 89%;
  display: inline-block;
}
.bb-vant-product-list .title-contril {
  border-bottom: 2rpx solid #E5E5E5;
  height: 70rpx;
  line-height: 70rpx;
  display:inline-block;
  width:100%;

}
.bb-vant-product-list .title-contril .title {
  font-family: PingFangSC-Regular;
  font-size: 33.75prx;
  color: #222222;
  text-align: left;
  display: inline-block;
  width: 80%;
}
.bb-vant-product-list .title-contril .subTitle {
  font-family: PingFang-SC-Medium;
  font-size: 26.25rpx;
  color: #F5A623;
  text-align: right;
  display: inline-block;
  width: 16%;
}
.bb-vant-product-list .title-contril .van-checkbox__label {
  display: inline-block;
  width: 89%;
}
.bb-vant-product-list .title-contril .van-checkbox__icon {
  margin-bottom: 0;
}
.bb-vant-product-list .checkbox-group-list {
  margin-top: 10rpx;
  background: #fff;
}
.bb-vant-product-list .submit-bar {
  width:100%;
  height: 100rpx;
  line-height: 100rpx;
  position: fixed;
  bottom:0;
  background: #fff;
  box-shadow: inset 0 2rpx 0 0 rgba(0,0,0,0.50);
}
.bb-vant-product-list .submit-bar .submit-bar-left{
  display: inline-block;
  width: 70%;
}
.bb-vant-product-list .submit-bar .submit-bar-left .select-button{
  font-family: PingFang-SC-Regular;
  font-size: 30rpx;
  color: #333A40;
  text-align: left;
  line-height: 100rpx;
  position:relative;
  bottom:10rpx;

}
.bb-vant-product-list .submit-bar .submit-bar-left .price{
  font-family: PingFang-SC-Medium;
  font-size: 38rpx;
  color: #F5A623;
  line-height: 100rpx;
  text-align: right;
  float: right;
  margin-right:38rpx; 
}
.bb-vant-product-list .submit-bar .van-checkbox__label {
  display: inline-block;
  width: auto;
}
.bb-vant-product-list .submit-bar .van-checkbox__icon {
  margin-bottom: 5rpx;
}
.bb-vant-product-list .submit-bar .submit-button{
  display: inline-block;
  width: 30%;
  background: #F5A623;
}
.bb-vant-product-list .submit-bar .submit-button button{
  display: inline-block;
  width: 100%;
  background: #F5A623;
  color: #fff;
  border:0;
  box-shadow:none;
  font-family: PingFang-SC-Medium;
  line-height:104rpx;
  /* font-size: 34rpx;
  text-align: center; */
}
.checked{
  display: inline-block;
  border-radius: 100%;
  width: 40rpx;
  height: 40rpx;
  color: #06bf04;
  border-color: #06bf04;
  background-color: #fff;
  top:5rpx;
  position:relative;
  margin:0 20rpx;
  line-height:40rpx;
  font-size: 40rpx;
}
.unchecked{
  display: inline-block;
  border-radius: 100%;
  font-size: 24rpx;
  color: transparent;
  text-align: center;
  border: 2rpx solid #aaa;
  width: 40rpx;
  height: 40rpx;
  box-sizing: border-box;
  top:5rpx;
  position:relative;
  margin:0 20rpx;
  line-height:40rpx;
}